{
  "name": "@open-hims/screens-web",
  "version": "1.0.0",
  "description": "Shared screens and components for web and desktop platforms using Mantine",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@open-hims/types": "workspace:*",
    "@open-hims/store": "workspace:*",
    "react-router-dom": "^6.28.0",
    "react-redux": "^8.1.3"
  },
  "devDependencies": {
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "typescript": "^5.6.3",
    "tsup": "^8.3.5"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "@mantine/core": ">=7.0.0",
    "@mantine/hooks": ">=7.0.0"
  },
  "publishConfig": {
    "access": "restricted"
  }
}